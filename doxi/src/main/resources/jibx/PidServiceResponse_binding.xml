<?xml version="1.0" encoding="UTF-8"?>
<binding>
	<!-- concrete mapping for PidServiceResponseVO -->
	<mapping name="pidserviceresponse" class="de.mpg.mpdl.doxi.pidcache.PidServiceResponseVO" ordered="false">
		
		<value style="element" name="action" usage="optional" field="action"/>
		<value style="element" name="matches" usage="optional" field="matches"></value>
		<value style="element" name="message" usage="optional" field="message"></value>
		
		<structure name="Handle" usage="optional">
			<value style="element" name="pid" usage="optional" field="identifier"/>
			<value style="element" name="url" usage="optional" field="url"/>
			<value style="element" name="creator" usage="optional" field="creator"/>
		</structure>
		
		<structure name="User" usage="optional">
			<value style="element" name="uid" usage="optional" field="userUid"/>
			<value style="element" name="institute" usage="optional" field="institute"/>
			<value style="element" name="contact" usage="optional" field="contact"/>
		</structure>
	</mapping>
</binding>
